{
  "name": "@8xm/sdk",
  "version": "1.0.0",
  "description": "8xM Token Launchpad SDK - Integrate token minting into any project",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./server": {
      "require": "./dist/server.js",
      "import": "./dist/server.mjs",
      "types": "./dist/server.d.ts"
    },
    "./client": {
      "require": "./dist/client.js",
      "import": "./dist/client.mjs",
      "types": "./dist/client.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/server.ts src/client.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/server.ts src/client.ts --format cjs,esm --dts --watch"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.10.4",
    "@solana/spl-token": "^0.4.13",
    "@solana/spl-token-metadata": "^0.1.6",
    "@solana/web3.js": "^1.98.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "express": "^4.18.0",
    "hono": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "express": { "optional": true },
    "hono": { "optional": true }
  },
  "keywords": ["solana", "token", "launchpad", "8xm", "quillverse", "web3"],
  "author": "TimeKnot Games",
  "license": "MIT"
}
